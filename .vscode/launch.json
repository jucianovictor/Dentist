{
	"version": "0.1.0",
	"configurations": [
		{
			"name": "Next.js: debug",
			"type": "node-terminal",
			"request": "launch",
			"command": "npm run dev",
			"console": "integratedTerminal",
			"serverReadyAction": {
				"pattern": "started server on .+, url: (https?://.+)",
				"uriFormat": "%s",
				"action": "debugWithEdge"
			},
			"webRoot": "${workspaceFolder}"
		}
	]
}
